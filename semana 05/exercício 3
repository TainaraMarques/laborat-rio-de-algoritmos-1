n1 = float (input("informe sua nota "))
n2 = float (input("informe sua outra nota "))
m = (n1+n2) / 2
f = int(input("informe sua frequência "))

if m >= 7 and f >= 75:
    print ("aprovado")

elif m >= 4 and m < 7 and f >= 75:
    print ("você ficou em exame")

elif m < 4 or f < 75:
    print ("reprovado")
