n1 = float (input("informe sua nota "))
n2 = float (input("informe sua outra nota "))
m = (n1+n2) / 2
if m >= 9:
    print ("sua média de aproveitamento é A")
    mp = "a"

elif m >= 7.5:
    print ("sua média de aproveitamento é B")
    print ("você foi aprovado")

elif m >= 6:
    print ("sua média de aproveitamento é C")
    print ("você foi aprovado")

elif m >= 4:
    print ("sua média de aproveitamneto é D")
    print ("você foi reprovado")

else:
    print ("sua média de aproveitamento é E")
    print ("você foi reprovado")





