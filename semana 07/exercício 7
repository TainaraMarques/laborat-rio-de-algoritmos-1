resultado = 1 
base = float(input("Digite a base: "))
expoente = int(input("Digite o expoente: "))
calculo = 0

if expoente < 0:
        base = 1 / base
        expoente = -expoente

expoenteoriginal = expoente 
while expoente > 0:
    resultado *= base
    expoente -= 1

print(base," elevado a" ,expoenteoriginal, "Ã© igual a", resultado)
