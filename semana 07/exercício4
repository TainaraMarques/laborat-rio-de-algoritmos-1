variavel = 77
numerousuario = int(input('digite um numero entre 1 a 100 '))

while numerousuario != variavel:
    if numerousuario > variavel:
        print ('tente um numero menor')

    elif numerousuario < variavel:
        print ('tente um numero maior')

    numerousuario = int(input('Digite outro número: '))
    
print('Parabéns!!! você acertou o número')
