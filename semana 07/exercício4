num = int(input("Digite um número inteiro positivo: "))

# Verifica se o número é maior que 1
if num > 1:
    # Inicializa o divisor como 2
    divisor = 2

    while divisor < num:
        if num % divisor == 0:
            print(f"{num} não é um número primo.")
            break
        divisor += 1
    else:
        print('esse é um número primo.')
        
# Se o número for menor ou igual a 1, não é primo
else:
    print('esse não é um número primo.')
